{
  "name": "asr-web-portal-frontend",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite --port 3000 --host",
    "build": "vite build",
    "preview": "vite preview",
    "prettier": "prettier --check .",
    "pretty-quick": "pretty-quick",
    "prettier-fix": "prettier --write .",
